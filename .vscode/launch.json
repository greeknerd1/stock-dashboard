{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug Frontend",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/frontend/src",
        "preLaunchTask": "Start Frontend"
      },
      {
        "name": "Debug Backend",
        "type": "node",
        "request": "launch",
        "program": "${workspaceFolder}/backend/server.js",
        "cwd": "${workspaceFolder}/backend"
      },
    ],
    "compounds": [
    {
      "name": "Debug Both",
      "configurations": ["Debug Backend", "Debug Frontend"]
    }
  ]
  }
  